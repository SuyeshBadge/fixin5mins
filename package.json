{
  "name": "fixin5mins",
  "version": "1.0.0",
  "description": "AI-powered Instagram content generator",
  "main": "dist/index.js",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "build": "tsc",
    "schedule-posts": "ts-node src/jobs/dailyPostScheduler.ts",
    "start": "docker-compose up -d",
    "stop": "docker-compose down",
    "logs": "docker-compose logs -f",
    "build-docker": "docker-compose up --build -d",
    "generate-and-post": "ts-node src/jobs/generateAndPostContent.ts"
  },
  "keywords": [
    "ai",
    "instagram",
    "content",
    "generation"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.8",
    "cloudinary": "^1.41.3",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.10",

    "form-data": "^4.0.0",
    "node-cron": "^3.0.3",
    "node-fetch": "^2.7.0",
    "openai": "^4.11.0",
    "puppeteer": "^24.6.1",
    "uuid": "^9.0.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/ejs": "^3.1.5",

    "@types/form-data": "^2.5.2",
    "@types/node": "^20.8.2",
    "@types/node-cron": "^3.0.11",
    "@types/puppeteer": "^5.4.7",
    "@types/uuid": "^9.0.8",
    "@types/winston": "^2.4.4",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
